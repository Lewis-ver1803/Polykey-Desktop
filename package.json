{
  "name": "polykey",
  "productName": "Polykey",
  "version": "0.0.1",
  "private": true,
  "description": "A peer-to-peer secret sharing and management application",
  "author": "Roger Qiu",
  "contributors": [
    "Aaashwin Varshney",
    "Gideon Rosales",
    "Robert Cronin"
  ],
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint '{src,tests}/**/*.{js,ts}'",
    "lintfix": "eslint '{src,tests}/**/*.{js,ts}' --fix",
    "vue-devtools": "vue-devtools",
    "npm-run-all": "npm-run-all",
    "rimraf": "rimraf",
    "webpack-cli": "webpack-cli",
    "compile": "npm-run-all webpack-cli mock_for_oauth2orize",
    "dev:webpack:watch": "webpack-cli --watch",
    "dev:electron": "electron .",
    "cleanup": "rimraf ./dist",
    "mock_for_oauth2orize": "mkdir -p ./dist/grant && mkdir -p ./dist/exchange",
    "test": "jest",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make:mac": "electron-forge make --platform darwin",
    "make:win": "electron-forge make --platform win32",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@matrixai/polykey": "0.0.33",
    "cyclist": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "file-saver": "^2.0.5",
    "fix-path": "^3.0.0",
    "ts-node": "^8.8.2",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^5.1.9",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@grpc/grpc-js": "^1.2.5",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/jest": "^25.0.0",
    "@types/node": "^13.13.1",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^4.15.0",
    "@vue/cli": "^4.5.8",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/devtools": "^6.0.0-beta.2",
    "@vue/test-utils": "^2.0.0-beta.5",
    "ant-design-vue": "^2.0.0-rc.8",
    "copy-webpack-plugin": "^6.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.9.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.0.0-0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jdenticon": "^3.1.0",
    "jest": "^25.0.0",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-loader": "^4.0.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "ts-loader": "^8.0.7",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "~3.9.3",
    "unzip": "^0.1.11",
    "vue": "^3.0.5",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.0.0-beta.5",
    "vue-router": "^4.0.3",
    "vue-style-loader": "^4.1.2",
    "vue-svg-loader": "^0.17.0-beta.2",
    "vuex": "^4.0.0-rc.2",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.2.0",
    "webpackbar": "^4.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/MatrixAI/PolyKey/issues"
  },
  "build": {
    "electronVersion": "9.0.0",
    "appId": "io.polykey.polykey",
    "extends": null,
    "files": [
      "dist/**/*",
      "static"
    ],
    "linux": {
      "icon": "static/logo.png",
      "target": "tar.gz"
    },
    "win": {
      "icon": "static/logo.png",
      "target": "portable"
    },
    "mac": {
      "icon": "static/logo-big.png",
      "target": "tar.gz"
    }
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "homepage": "https://github.com/MatrixAI/PolyKey",
  "keywords": [
    "secrets"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/PolyKey.git"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "osxSign": {
          "identity": "Developer ID Application: MATRIX AI PTY. LTD. (9347PACJQG)",
          "hardened-runtime": true,
          "entitlements": "entitlements.plist",
          "entitlements-inherit": "entitlements.plist",
          "signature-flags": "library"
        },
        "osxNotarize": {
          "appleId": "gideon.rosales@matrix.ai",
          "appleIdPassword": "fvey-wjea-uyce-ktkn"
        },
        "icon": "icons/icons/mac/icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "iconUrl": "icons/icons/win/icon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./static/logo-big.png",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
